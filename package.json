{
  "name": "rectangle-drawer",
  "displayName": "Rectangle Drawer",
  "description": "Draw rectangles and connect them with mouse interactions",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rectangleDrawer.openDrawingView",
        "title": "Open Rectangle Drawer",
        "category": "Rectangle Drawer"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "rectangleDrawerView",
          "name": "Rectangle Drawer",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rectangleDrawer",
          "title": "Rectangle Drawer",
          "icon": "$(symbol-square)"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}